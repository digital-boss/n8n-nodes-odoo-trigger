{
	"name": "@digital-boss/n8n-nodes-odoo-trigger",
	"version": "1.1.0",
	"author": {
		"name": "IÃ±aki Breinbauer"
	},
	"license": "MIT",
	"description": "Odoo-trigger support for n8n",
	"keywords": [
		"n8n",
		"nodemation",
		"dnc",
		"digital north consulting",
		"digital boss",
		"helpdesk",
		"customer service",
		"nodes",
		"custom",
		"odoo-trigger",
		"workflow",
		"n8n-community-node-package"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/digital-boss/n8n-nodes-odoo-triggerv.git"
	},
	"main": "index.js",
	"scripts": {
		"dev": "npm run watch",
		"build": "genversion -s -e nodes/version.ts && git add . && tsc && gulp build:icons",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"nodelinter": "nodelinter --target=./nodes",
		"watch": "tsc --watch",
		"release": "npm run build && npm publish --access=public",
		"version": "npm run build",
		"postversion": "git push",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/OdooApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/OdooTrigger/OdooTrigger.node.js"
		]
	},
	"devDependencies": {
		"@types/express": "^4.17.2",
		"@types/request-promise-native": "~1.0.15",
		"@typescript-eslint/parser": "~5.45",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"genversion": "^3.1.1",
		"gulp": "^4.0.2",
		"n8n-core": "*",
		"n8n-workflow": "*",
		"nodelinter": "^0.1.19",
		"prettier": "^2.8.4",
		"typescript": "~4.8.4"
	},
	"bugs": {
		"url": "https://github.com/digital-boss/n8n-nodes-odoo-trigger/issues"
	},
	"homepage": "https://github.com/digital-boss/n8n-nodes-odoo-trigger#readme"
}
